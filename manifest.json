{
    "manifest_version": 2,
    "name": "Shift Google",
    "description": "This extension shifts the Google.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://www.google.co.jp/*", "https://www.google.com/*"],
            "css": ["shift.css"]
        }
    ]
}
